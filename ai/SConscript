Import('env')

sourceFiles = Split("""
   aiagent.cpp
   aiagentmessage.cpp
   aiagentmessagerouter.cpp
   aiagenttaskmoveto.cpp
   aiagenttaskstand.cpp
""")

libPaths = Split("""
   #allguids
   #tech
""")

linkLibs = Split("""
   allguids
   tech
""")

local = env.Copy()
local.BuildLibrary(target='ai',
                   source=sourceFiles,
                   lib_path=libPaths,
                   libs=linkLibs)
